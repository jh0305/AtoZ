package com.spring.AtoZ.item.dto;

public class UpdateStandardCommand {
	private int std_no;
	private String std_name;
	private int std_width;
	private int std_lngth;
	private int std_height;
	public int getStd_no() {
		return std_no;
	}
	public void setStd_no(int std_no) {
		this.std_no = std_no;
	}
	public String getStd_name() {
		return std_name;
	}
	public void setStd_name(String std_name) {
		this.std_name = std_name;
	}
	public int getStd_width() {
		return std_width;
	}
	public void setStd_width(int std_width) {
		this.std_width = std_width;
	}
	public int getStd_lngth() {
		return std_lngth;
	}
	public void setStd_lngth(int std_lngth) {
		this.std_lngth = std_lngth;
	}
	public int getStd_height() {
		return std_height;
	}
	public void setStd_height(int std_height) {
		this.std_height = std_height;
	}
	@Override
	public String toString() {
		return "UpdateStandardCommand [std_no=" + std_no + ", std_name=" + std_name + ", std_width=" + std_width
				+ ", std_lngth=" + std_lngth + ", std_height=" + std_height + "]";
	}
}
